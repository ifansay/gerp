  ads                                Int
  holiday_reply_email                Int
  ignore_order_invitation_date_befor DateTime
  order_incitation_email2            Int
  add_date                           DateTime
  others                             String
  /// 禁用子账号
  disable_Subaccounts                Int
  /// 广告授权码
  ads_code                           String
  /// 授权码状态
  ads_state                          String
  /// 亚马逊授权状态 Invalid 无效, Normal 正常
  api_state                          String

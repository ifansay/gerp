generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

generator erd {
  provider     = "prisma-erd-generator"
  ignoreTables = ["_prisma_migrations", "Session", "Account", "Example"]
  disabled     = true
  ignoreEnums  = true
  output       = "database.svg"
}

model user {
  id                Int                 @id @default(autoincrement())
  biz_id            Int
  name              String
  e_listing_manager e_listing_manager[]
}

model e_listing_manager {
  id              Int       @id @default(autoincrement())
  listing_id      Int
  manager_id      Int
  manager_user_id Int
  user            user      @relation(fields: [manager_user_id], references: [biz_id])
}

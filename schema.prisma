  id                       Int      @id @default(autoincrement())
  /// 对账月份，格式：yyyy-MM
  reconc_month             String
  /// 对账类型，INIT_RECONC为FBA初始对账，MONTHLY_RECONC为FBA月度对账
  reconc_type              String

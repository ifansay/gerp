model e_listing_manager {
  id              Int       @id @default(autoincrement())
  listing_id      Int
  manager_id      Int
  manager_user_id Int
  e_listing       e_listing @relation(fields: [listing_id], references: [id])
}

model e_listing {
  id                  Int                 @id @default(autoincrement())
  market_id           Int
  name                String
  e_listing_manager   e_listing_manager[]
}

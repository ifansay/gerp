  api_state                 String
  /// VC店铺亚马逊中台公开token
  public_token              String
  /// VC店铺亚马逊中台sptoken
  refresh_token             String
  /// marketplace主键
  marketplace_id            String
  /// 时间区域
  time_zone                 String
  /// asin_url地址
  asin_url                  String
  /// 创建时间
  create_time               DateTime
  /// 创建人ID
  create_user_id            Int
  /// 更新人ID
  update_user_id            Int
  /// 更新时间
  update_time               DateTime
  /// 0.未删除 1.删除
  delete_state              Int
  /// 删除时间
  delete_time               DateTime
  /// 删除人ID
  delete_user_id            Int
  /// 广告授权码
  ads_code                  String
  /// 授权码状态
  ads_state                 String
  /// 仓库ID
  warehouse_id              Int
  /// 关联的服务器ID
  server_id                 Int
  /// 子账号
  account                   String
  /// 子账号密码
  pwd                       String
  /// 子账号二次验证密钥
  auth_secret_key           String
  /// 标识是否启用加密 0.未加密 1.已加密 2.已修改 默认0
  encrypt_flag              Int
  /// 子账号session 0：NO 1：YES
  session                   Int
  /// 禁用子账号 0：可用 1：禁用
  disable_subaccounts       Int
  /// 子账号禁用原因
  subaccount_disable_reason String
  e_market_polymerize       e_market_polymerize[]
}

